    "@types/node": "^18.8.5",
    "@types/nodemailer": "^6.4.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.39.0",
    "@vitest/coverage-c8": "^0.24.3",
    "eslint": "^8.24.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  },
  "dependencies": {
    "@abraham/reflection": "^0.11.0",
    "@fastify/cookie": "^8.3.0",
