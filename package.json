{
  "name": "@norther/core",
  "version": "1.0.0-alpha.1",
  "description": "The modern Node.js framework",
  "type": "module",
  "main": "./dist/module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northerjs/core.git"
  },
  "author": "Dominik Rajkowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/northerjs/core/issues"
  },
  "engines": {
    "node": ">=18.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/module"
  },
  "bin": {
    "norther": "dist/.bin/norther.js"
