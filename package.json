    "bcrypt": "^5.1.0",
    "chalk": "^5.1.2",
    "chokidar": "^3.5.3",
    "concurrently": "^7.4.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.8.1",
    "nodemailer": "^6.8.0",
    "socket.io": "^4.5.2",
    "strip-ansi": "^7.0.1",
    "tslib": "^2.4.0"
