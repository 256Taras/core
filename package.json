{
  "name": "@northle/core",
  "version": "1.0.0-alpha.1",
  "description": "Modern, declarative framework for Node.js",
  "type": "module",
  "main": "./dist/main",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/northle/core.git"
  },
  "author": "Dominik Rajkowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/northle/core/issues"
  },
  "engines": {
    "node": ">=18.3.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/main"
  },
  "bin": {
    "northle": "bin/northle.js"
  },
  "scripts": {
    "build": "tsc",
