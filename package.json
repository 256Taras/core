    "@types/bcrypt": "^5.0.0",
    "@types/node": "^18.7.4",
    "@types/semver": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitest/coverage-c8": "^0.22.0",
    "eslint": "^8.22.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "vitest": "^0.22.0"
  },
  "dependencies": {
